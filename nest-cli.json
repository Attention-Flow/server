{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/sma_services/src",
  "monorepo": true,
  "root": "apps/sma_services",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/sma_services/tsconfig.app.json"
  },
  "projects": {
    "sma_server": {
      "type": "application",
      "root": "apps/sma_server",
      "entryFile": "main",
      "sourceRoot": "apps/sma_server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sma_server/tsconfig.app.json"
      }
    },
    "sma_analysis": {
      "type": "library",
      "root": "libs/sma_analysis",
      "entryFile": "index",
      "sourceRoot": "libs/sma_analysis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sma_analysis/tsconfig.lib.json"
      }
    },
    "sma_groups": {
      "type": "library",
      "root": "libs/sma_groups",
      "entryFile": "index",
      "sourceRoot": "libs/sma_groups/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sma_groups/tsconfig.lib.json"
      }
    },
    "sma_messages": {
      "type": "library",
      "root": "libs/sma_messages",
      "entryFile": "index",
      "sourceRoot": "libs/sma_messages/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sma_messages/tsconfig.lib.json"
      }
    },
    "sma_common": {
      "type": "library",
      "root": "libs/sma_common",
      "entryFile": "index",
      "sourceRoot": "libs/sma_common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sma_common/tsconfig.lib.json"
      }
    },
    "sma_subscriptions": {
      "type": "library",
      "root": "libs/sma_subscriptions",
      "entryFile": "index",
      "sourceRoot": "libs/sma_subscriptions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sma_subscriptions/tsconfig.lib.json"
      }
    },
    "sma_analysis_v2": {
      "type": "library",
      "root": "libs/sma_analysis_v2",
      "entryFile": "index",
      "sourceRoot": "libs/sma_analysis_v2/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sma_analysis_v2/tsconfig.lib.json"
      }
    }
  }
}